{
  "crons": [
    {
      "path": "/api?endpoint=cron&path=all",
      "schedule": "0 9 * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api/|videos/|images/).*)",
      "destination": "/index.html"
    },
    {
      "source": "/api/cron/monitor",
      "destination": "/api?endpoint=cron&path=monitor"
    },
    {
      "source": "/api/email/check",
      "destination": "/api?endpoint=email"
    },
    {
      "source": "/api/cron/all",
      "destination": "/api?endpoint=cron&path=all"
    },
    {
      "source": "/api/ai-monitoring",
      "destination": "/api?endpoint=ai-monitoring"
    },
    {
      "source": "/api/deye/:path*",
      "destination": "/api?endpoint=deye&path=:path*"
    },
    {
      "source": "/api/auth/login",
      "destination": "/api?endpoint=auth"
    },
    {
      "source": "/api/auth/:path*",
      "destination": "/api?endpoint=auth"
    }
  ],
  "headers": [
    {
      "source": "/api/:path*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://sunterrasolarenergy.com"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,POST,PUT,DELETE,OPTIONS,PATCH"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type,Authorization,X-Requested-With,Accept,Origin"
        },
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Max-Age",
          "value": "86400"
        }
      ]
    }
  ]
}
